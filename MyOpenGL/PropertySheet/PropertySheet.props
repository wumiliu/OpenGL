<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <OutDir>$(SolutionDir)VS2013_Proj\Tmp\Link\$(ProjectName)\$(ConfigurationName)</OutDir>
    <IntDir>$(SolutionDir)VS2013_Proj\Tmp\Complie\$(ProjectName)\$(ConfigurationName)</IntDir>
    <IncludePath>$(IncludePath)</IncludePath>
    <LibraryPath>Dependencies/lib;$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>../jni/Engine;../jni/Engine/glm;Dependencies/include;../jni/Engine/BaseCode</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <OutputFile>$(SolutionDir)VS2013_Proj\Bin\$(ConfigurationName)\$(ProjectName).dll</OutputFile>
      <ImportLibrary>$(SolutionDir)VS2013_Proj\Lib\$(ConfigurationName)\$(TargetName).lib</ImportLibrary>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>